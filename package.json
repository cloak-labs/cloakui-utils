{
  "name": "@cloakui/utils",
  "version": "0.1.3",
  "description": "Shared utilities for all CloakUI component packages.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build": "npm run build-ts",
    "build-ts": "tsc -p tsconfig.json",
    "clean": "rm -rf .turbo && rm -rf dist && npm run clean:modules",
    "clean:modules": "rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloak-labs/cloakui-utils.git"
  },
  "keywords": [
    "ui",
    "utils",
    "utilities",
    "cloakui",
    "ui kit",
    "ui library"
  ],
  "author": "Kaelan Smith",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/cloak-labs/cloakui-utils/issues"
  },
  "homepage": "https://github.com/cloak-labs/cloakui-utils#readme",
  "devDependencies": {
    "@types/node": "^18.18.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}